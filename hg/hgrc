dlog = log --style=$HOME/lair/hg/hg-log/map-cmdline.dlog
nlog = log --style=$HOME/lair/hg/hg-log/map-cmdline.nlog
sglog = glog --style=$HOME/lair/hg/hg-log/map-cmdline.sglog